<table class="table is-fullwidth">
  <thead>
    <th>Jobvalue</th>
    <th>Method donated</th>
    <th>Donor</th>
    <th>Employee</th>
    <th>Image</th>
  </thead>
  <tbody>
    {{#each whereabouts}}
      <tr>
        <td>{{jobvalue}}</td>
        <td>{{method}}</td>
        <td> {{donor.firstName}} {{donor.lastName}} </td>
        <td>{{employee.lastName}}, {{employee.firstName}}</td>
        <td>{{> playlist-image }}</td>
      </tr>
    {{/each}}
  </tbody>
</table>
<div class="box">
  <div class="title is-5"> Total:  {{total}} </div>
</div>
